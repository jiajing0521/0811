<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>图片展示</title>
    <style>
        html,body{
            width: 100%;
        }
        img{
            width: 24%;
            height: 300px;
        }
    </style>
</head>
<body>
    <% images.forEach((item)=>{ %>
    <a href="/image/<%= item.id %>"><img src="<%= item.path %>" alt=""></a>
    <% }) %>
</body>
</html>